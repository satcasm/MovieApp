<h1>
	Results Page!!
</h1>

<% if (result == null) { %>
<p>Hmmmm....I didn't get that,please try something else!</p>
<% } else { %>
<ul>
   <% result.Search.forEach((movie)=>{ %>
		<li><strong><%= movie.Title %></strong> - <%= movie.Year %></li>
	<%});%>
</ul>
 
<% } %>
 
<a href="/">Search Again!</a>